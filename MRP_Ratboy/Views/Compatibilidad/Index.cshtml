        </div>
    </section>
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script>
        var cardComponent = "<div class='card' style='width: 16rem; margin: 5px; margin-left: 60px;'>" +
            "<img class='card-img-top'>" + 
            "<div class='card-body'>" + 
            "<h5 class='card-title' id='Nombre'></h5>" +
            "<p class='card-text'>Some quick example text to build on the card title and make up the bulk of the card'scontent.</p>" +
            "<input type='submit' class='btn btn-primary' id='btn-accept' value='Aceptar jejejeje' onclick='funciona()' /> </div> </div>";

        for (var i = 0; i < 5; i++) {
            $("#content-component-div").append(cardComponent);
        }
    </script>
</body>
</html>