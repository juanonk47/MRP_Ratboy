@model IEnumerable<MRP_Ratboy.Models.tarjetaVideo>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <link href="~/Content/css/estilos-compatible.css" rel="stylesheet" />
</head>
<body>
    <section class="card custom-card-content">
        <div class="col-md-12">
            <div class="row">
                <div class="col-md-2 process-lista">
                    <h1>1) Tarjeta Madre</h1>
